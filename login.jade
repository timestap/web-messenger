    body.container
      div.signin
        form.form-group(id='myform', action='', method="post")
          h2.form-signin-heading Please enter your username
          label.sr-only(for='usrname') Username
          input.form-control(type='TEXT', name='username', placeholder='Username')
          button.btn.btn-lg.btn-primary.btn-block(type='submit') Enter Chatroom
      script. 
        document.cookie = 'username=; expires=Thu, 01 Jan 1970 00:00:01 GMT;';



    


